{
  "name": "Test-task",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "description": "Test task developed by Karen Gasparyan",
  "author": "Karen Gasparyan",
  "scripts": {
    "format": "npx prettier . --write",
    "start": "node dist/index.js",
    "build": "rimraf dist && npx tsc",
    "dev": "nodemon",
    "migration:create:db": "sequelize-cli db:create",
    "migration:generate": "npx sequelize migration:generate",
    "migration:run": "npx sequelize db:migrate",
    "migration:undo": "npx sequelize db:migrate:undo",
    "migration:revert": "npx sequelize db:migrate:undo:all",
    "seed:run": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:revert": "sequelize db:seed:undo:all"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.15",
    "pg": "^8.8.0",
    "prettier": "^3.3.2",
    "redis": "^4.6.13",
    "sequelize": "^6.28.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.0",
    "yamljs": "^0.3.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.2.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.16.19",
    "@types/nodemailer": "^6.4.15",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@types/yamljs": "^0.2.31",
    "nodemon": "^3.0.3",
    "prettier-standard": "^15.0.1",
    "sequelize-cli": "^6.6.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=v16.11.35",
    "npm": ">=7.20.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": [
      "eslint --cache . --ext ts,tsx --fix",
      "prettier --write ."
    ]
  }
}
